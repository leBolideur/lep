fn reduce(arr, initial, f):
    fn iter(arr, result):
        if (len(arr) == 0):
            ret result;
        else:
            iter(tail(arr), f(result, head(arr)));
        end
    end

     iter(arr, initial);
end;

var sum = fn(arr):
    reduce(arr, 0, fn(initial, el): initial + el end);
end;

