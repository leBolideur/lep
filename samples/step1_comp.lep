var my_number = 1 + 20;
var second_number = 29;
var result = my_number + second_number;

var ff = fn(): ret 1 + 1; end;

var arr = [1, 3, 9];
var hash = {"author": "Max", "lang": 1};

if (result > arr[2]):
   hash["author"];
ff();
else:
    hash["lang"];
end

var fibonacci = fn(x) :
    if (x == 0) :
        ret 0;
    else :
        if (x == 1) :
            ret 1;
        else :
            fibonacci(x - 1) + fibonacci(x - 2);
        end
    end
end;

fibonacci(10);
